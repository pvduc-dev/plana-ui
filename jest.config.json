{
  "testEnvironment": "jest-environment-jsdom",
  "setupFilesAfterEnv": [
    "<rootDir>/setupTest.ts"
  ],
  "transform": {
    "\\.(ts|tsx)$": "babel-jest",
    "^.+\\.css$": [
      "jest-transform-css",
      {
        "modules": true
      }
    ]
  },
  "collectCoverage": false,
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}"
  ]
}
